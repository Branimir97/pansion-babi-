{% extends 'base.html.twig' %}

{% block meta %}
<meta name="description" content="{{ 'gallery'|trans({}, 'meta_description')|raw}}">
{% endblock %}

{% block hreflang %}
    <link rel="alternate" href="{{ prod_url }}hr/{{ app.request.attributes.get('_route') }}" hreflang="hr"/>
    <link rel="alternate" href="{{ prod_url }}en/{{ app.request.attributes.get('_route') }}" hreflang="en-gb"/>
    <link rel="alternate" href="{{ prod_url }}it/{{ app.request.attributes.get('_route') }}" hreflang="it"/>
    <link rel="alternate" href="{{ prod_url }}de/{{ app.request.attributes.get('_route') }}" hreflang="de"/>
{% endblock %}

{% block title %}
    {{ 'title'|trans({}, 'gallery')}}
{% endblock %}

{% block javascripts %}
